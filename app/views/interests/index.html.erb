<%= render :partial => "interests/menu" %>
<h1> Zarządzanie zainteresowaniami </h1>

<span class="paginate">

<%= will_paginate @interests, :params => {:search => params[:search]} %></span>

<br />
<table id="TabFullSize">
  <tr>
    <td class="header">Nazwa</td>
    <td class="header">Usuń</td>
  </tr>
    <% @interests.each do |interest| %>
    <tr>
            <td><%= interest.name%></td>
            <td class="center">
                    <%= link_to image_tag("delete.png", :border=>0), {:action => "destroy", :id => interest.id} %>
            </td>
</tr>
    <% end %>

</table>


